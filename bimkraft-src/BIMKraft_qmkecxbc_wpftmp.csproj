    <AssemblyName>BIMKraft</AssemblyName>
    <IntermediateOutputPath>obj\x64\Debug\</IntermediateOutputPath>
    <BaseIntermediateOutputPath>obj\</BaseIntermediateOutputPath>
    <MSBuildProjectExtensionsPath>D:\repos\manishpaulsimon\bimkraft\bimkraft-src\obj\</MSBuildProjectExtensionsPath>
